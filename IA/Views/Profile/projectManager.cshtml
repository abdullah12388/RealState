@model IA.Models.users
@{
    ViewBag.Title = "Profile";
    ViewBag.user = Model;
    Layout = "~/Views/Shared/_AdminProfile.cshtml";

}
<style>
p input{
    margin-bottom:5px;
}
</style>
<p>
    <input type="button" class="btn btn-primary btn-sm" id="e_b" value="Experience" />
    <input type="button" class="btn btn-primary btn-sm" id="customerRequest" value="Request" />
    <input type="button" class="btn btn-primary btn-sm" id="team" value="Team" />
    <input type="button" class="btn btn-primary btn-sm" id="c_b" value="Current" />
    <input type="button" class="btn btn-primary btn-sm" id="control" onclick="" value="Control" />
    <input type="button" class="btn btn-primary btn-sm" id="schedule" value="Schedule" />
    <input type="button" class="btn btn-primary btn-sm" id="report" value="Reports" />
    <input type="button" class="btn btn-primary btn-sm" id="com" onclick="comment()" value="Comments" />
    <input type="button" class="btn btn-primary btn-sm" id="p_b" value="Delivered" />
</p>

<script type="text/javascript" src="https://www.google.com/jsapi"></script>
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>


<div id="state"></div>

<script type="text/javascript">
    $("#e_b").click(function () {
        $('#state').empty();
        $.get('@Url.Action("Exp_chart", "ProjectManager")', {}, function (response) {
        $("#state").html(response);
         });
    });
</script>

<script type="text/javascript">
    $("#c_b").click(function () {
        $('#state').empty();
      $.get('@Url.Action("cur_projects", "ProjectManager")', {}, function (response) {
        $("#state").html(response);
         });
    });
</script>

<script type="text/javascript">
    $("#p_b").click(function () {
        $('#state').empty();
      $.get('@Url.Action("pre_project", "ProjectManager")', {}, function (response) {
        $("#state").html(response);
         });
    });
</script>

<script type="text/javascript">
    $("#control").click(function () {
        $('#state').empty();
      $.get('@Url.Action("cur_projects_list", "ProjectManager")', {}, function (response) {
        $("#state").html(response);
        });
    });
</script>

<script type="text/javascript">
    function comment() {
        $('#state').empty();
        var url = '@Url.Action("comment", "ProjectManager")';
        $.ajax({
            url: url,
            type: 'GET',
            cache: false,
            success: function (result) {
                $('#state').html(result);
            }
        });
    }
</script>

<script type="text/javascript">
    $("#customerRequest").click(function () {
        $('#state').empty();
      $.get('@Url.Action("RequestView", "ProjectManager")', {}, function (response) {
        $("#state").html(response);
        });
    });
</script>

<script type="text/javascript">
    $("#team").click(function () {
        $('#state').empty();
      $.get('@Url.Action("TeamView", "ProjectManager")', {}, function (response) {
        $("#state").html(response);
        });
    });
</script>

<script type="text/javascript">
    $("#report").click(function () {
        $('#state').empty();
      $.get('@Url.Action("ReportView", "ProjectManager")', {}, function (response) {
        $("#state").html(response);
        });
    });
</script>

<script type="text/javascript">
    $("#schedule").click(function () {
        $('#state').empty();
      $.get('@Url.Action("ScheduleView", "ProjectManager")', {}, function (response) {
        $("#state").html(response);
        });
    });
</script>
